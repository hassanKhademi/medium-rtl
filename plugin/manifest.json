{
  "name": "راست چین سگه برای تیم پادسایت",
  "version": "0.1",
  "manifest_version": 3,
  "minimum_chrome_version": "49.0",
  "description": "Set RTL support for poe.com",
  "icons": { "16": "icon.png", "48": "icon.png", "128": "icon.png" },
  "content_scripts": [
    {
      "matches": ["https://poe.com/*"],
      "js": ["content-script.js"]
    }
  ]
}
